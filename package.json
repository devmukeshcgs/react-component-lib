{
  "name": "bm-react-component-lib",
  "version": "0.1.2",
  "private": false,
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "dependencies": {
    "web-vitals": "^2.1.4",
    "styled-components": "*"
  },
  "scripts": {
    "compile-icons": "npx @svgr/cli -d src/icons src/icons --typescript",
    "copy-files": "copyfiles --up 1 src/config/fonts/* lib/",
    "start": "react-scripts start",
    "build": "tsc",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prebuild": "rimraf ./lib && npm run compile-icons && npm run copy-files",
    "prepublish": "npm run build "
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "peerDependencies": {
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.29",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@storybook/addon-essentials": "^7.0.11",
    "@storybook/addon-interactions": "^7.0.11",
    "@storybook/addon-links": "^7.0.11",
    "@storybook/blocks": "^7.0.11",
    "@storybook/preset-create-react-app": "^7.0.11",
    "@storybook/react": "^7.0.11",
    "@storybook/react-webpack5": "^7.0.11",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@svgr/cli": "^8.0.1",
    "@types/styled-components": "^5.1.26",
    "babel-plugin-named-exports-order": "^0.0.2",
    "copyfiles": "^2.4.1",
    "prop-types": "^15.8.1",
    "storybook": "^7.0.11",
    "styled-components": "*",
    "typescript": "^4.9.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "webpack": "^5.82.1"
  }
}